{
    "name": "satnetuser001/hello-world",
    "description": "This is an application for learning class autoloading using Composer (PSR-4)",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "fakerphp/faker": "^1.23"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "Anton Yurchenko",
            "email": "satnetuser001@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/satnetuser001/hello-world"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
